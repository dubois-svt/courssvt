{
    "cSpell.language": "fr,fr-FR,en-US,en-GB",
    "grammarly.selectors": [
        {
            "language": "markdown",
            "scheme": "file"
        }
    ],
    "cSpell.customDictionaries": {
        "custom-dictionary-workspace": {
            "name": "custom-dictionary-workspace",
            "path": "${workspaceFolder:courssvt}/.cspell/custom-dictionary-workspace.txt",
            "addWords": true,
            "scope": "workspace"
        }
    },
    "languageToolLinter.external.url": "https://ortho.kaeell.fr/v2",
    "languageToolLinter.plainText.languageIds": [
        "plaintext",
        "markdown"
    ],
    "languageToolLinter.serviceType": "external"
}